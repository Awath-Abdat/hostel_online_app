<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dialog_backgroud"
    android:id="@+id/relativeLayout"
    android:padding="16dp"
    tools:context=".StudentNotifications">

    <include layout="@layout/content_student_notifications" />

    <Button
        android:id="@+id/button2"
        style="@style/button_light_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="111dp"
        android:layout_marginBottom="29dp"
        android:fontFamily="@font/segoeui"
        android:onClick="newNotification"
        android:text="NEW NOTIFICATION" />


</RelativeLayout>